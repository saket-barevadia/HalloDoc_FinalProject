                            <select asp-for="@Model.Status" value="@Model.Status" class="form-select form-control d1class" disabled id="status">
                                <option value="1">Active</option>
                                <option value="2">Pending</option>
                                <option value="3">Not Active</option>
                            </select>
                            <label for="status" class="ms-2 ">Status</label>
                        </div>
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12  form-floating mb-3 ">
                            <select class="form-select form-control d1class" asp-for="Roleid" disabled id="Role" value="@Model.Roleid">
                                @foreach(var item in Model.roles)
                                {
                                    <option value="@item.Roleid">@item.role</option>
                                }                                
                                
                            </select>

                            <label for="Role" class="ms-2 ">Role</label>
                            <span asp-validation-for="Roleid" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="d-flex justify-content-end mt-3">
                        <button type="button" id="editBtn1" class="btn btn-info text-white EditBtn"
                                onclick="enableFields(1)">
                            Edit
                        </button>

                        <button type="submit" id="submitBtn1" class="btn btn-info text-white SaveBtn me-2">Save</button>

                        <button type="button" id="cancelBtn1" class="btn btn-outline-info CancelBtn"
                                onclick="disableFields(1)">
                            Cancel
                        </button>
                        <button type="button" onclick="resetPassword()" id="ResetPassword"
                                class="btn btn-outline-info ms-2">
                            Reset Password
                        </button>
                    </div>
                </div>
            </form>

