<script>


    
   


    function patient(id, status) {
        $.ajax({
            url: "/Provider/ProviderTable",
            type: "GET",
            data: { status:status, reqTypeId: id },
            success: function (data) {
                $('#tableContentt').html(data);

            }
        })
    }

    function friend(id, status) {
        $.ajax({
            url: "/Provider/ProviderTable",
            type: "GET",
            data: { status: status, reqTypeId: id },
            success: function (data) {
                $('#tableContentt').html(data);

            }
        })
    }

    function business(id, status) {
        $.ajax({
            url: "/Provider/ProviderTable",
            type: "GET",
            data: { status: status, reqTypeId: id },
            success: function (data) {
                $('#tableContentt').html(data);

            }
        })
    }

    function concierge(id, status) {
        $.ajax({
            url: "/Provider/ProviderTable",
            type: "GET",
            data: { status: status, reqTypeId: id },
            success: function (data) {
                $('#tableContentt').html(data);

            }
        })
    }
    function forAll(id,status) {
        $.ajax({
            url: "/Provider/ProviderTable",
            type: "GET",
            data: { status: status, reqTypeId: id },
            success: function (data) {
                $('#tableContentt').html(data);

            }
        })
    }







    // Search

    $(document).ready(function () {

        $('.providerTableData').DataTable({
            "initComplete": function (settings, json) {

                $('#my-search-input').val(settings.oPreviousSearch.sSearch);

                $('#my-search-input').on('keyup', function () {
                    var searchValue = $(this).val();
                    settings.oPreviousSearch.sSearch = searchValue;
                    settings.oApi._fnReDraw(settings);
                });
            },
            "lengthMenu": [[5, 10, -1], [5, 10, "All"]],
            "pageLength": 5,
          
            language: {
                oPaginate: {
                    sNext: '<i class="fa-solid fa-forward text-info"></i>',
                    sPrevious: '<i class="fa-solid fa-backward text-info"></i>'

                }
            }
        });
        // $('.dataTables_length').hide();
        $('.dataTables_filter').hide();
    });

    document.getElementById('my-search-input').addEventListener('keydown', function (event) {
        if (event.key === 'Enter') {
            event.preventDefault();
            this.blur();
        }
    });




    function ProviderTransferRequest(reqId) {
        $.ajax({
            method: "Get",
            url: "/Provider/TransferRequest",
            traditional: true,
            contentType: "application/json",
            data: { requestId: reqId },
            success: function (result) {
                $('#offCanvass').html(result);
                $('#TranferModal').modal('show');
            },
            error: function () {
                alert("error");
            },
        });
    }



    function transferReqPost() {
        event.preventDefault();
        if ($('#TranferRequestForm').valid()) {
            $.ajax({
                method: "POST",
                url: "/Provider/TransferReqPost",
                data: $('#TranferRequestForm').serialize(),
                success: function () {

                    $('#TranferModal').modal('hide');
                    toastr.success("Request Transferred Successfully");
                    getContent();
                },
                error: function () {
                    toastr.error("Something Went Wrong");
                }
            });
        }
    }


    function createRequest(){
        $.ajax({
            method: "Get",
            url: "/Provider/createRequest",
            traditional: true,
            contentType: "application/json",
            data: { flag: 2 },
            success: function (result) {
                $('#tabContents').html(result);
               
            },
            error: function () {
                alert("error");
            },
        });
    }




    function sendLinkModal() {
        $.ajax({
            method: "Get",
            url: "/Provider/SendLink",
            traditional: true,
            contentType: "application/json",          
            success: function (result) {
                $('#offCanvass').html(result);
                $('#sendLinkModal').modal('show');
            },
            error: function () {
                alert("error");
            },
        });
    }




    function sendLinkPost() {
        event.preventDefault();
        if ($('#sendLinkForm').valid()) {
            $.ajax({
                method: "POST",
                url: "/Provider/sendLinkPost",
                data: $('#sendLinkForm').serialize(),
                success: function () {

                    $('#sendLinkModal').modal('hide');
                    toastr.success("Link Send Successfully");

                },
                error: function () {
                    toastr.error("Something Went Wrong");
                }
            });
        }
    }



    function viewCase(reqId) {
        $.ajax({
            method: "Get",
            url: "/Provider/ViewCase",
            traditional: true,
            contentType: "application/json",
            data: { id: reqId },
            success: function (result) {
                $('#tabContents').html(result);

            },
            error: function () {
                alert("error");
            },
        });
    }


    function viewNotes(reqId) {
        $.ajax({
            method: "Get",
            url: "/Provider/ViewNotes",
            traditional: true,
            contentType: "application/json",
            data: { id: reqId },
            success: function (result) {
                $('#tabContents').html(result);

            },
            error: function () {
                alert("error");
            },
        });
    }



    function AcceptRequest(reqId) {
        $.ajax({
            method: "Get",
            url: "/Provider/Accept",
            traditional: true,
            contentType: "application/json",          
            success: function (result) {
                $('#offCanvass').html(result);
                $('#AcceptProvider').modal('show');
                $('#AcceptProviderForm').val(reqId);
            },
            error: function () {
                alert("error");
            },
        });
    }


    function AcceptRequestPost(Requestid) {
        event.preventDefault();
        $.ajax({
            method: "POST",
            url: "/Provider/AcceptRequest",
            data: { Requestid: Requestid },
            success: function (result) {
                if (result.code == 401) {
                    setTimeout(function () { location.reload(); }, 2000);
                    toastr.error("Something Went Wrong");
                }
                else {
                    $('#AcceptProvider').modal('hide');                  
                    toastr.success("Request Accepted");
                    getContent();
                }
            },
            error: function () {
                toastr.error("Something Went Wrong");
            }
        });
    }



    function SendAgreement(reqId) {
        $.ajax({
            method: "Get",
